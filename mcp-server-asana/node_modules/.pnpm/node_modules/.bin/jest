#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/adamhayden/Documents/Apps/Quibit_RAG/ET_v001/mcp-server-asana/node_modules/.pnpm/jest-cli@29.7.0_@types+node@20.19.1/node_modules/jest-cli/bin/node_modules:/Users/adamhayden/Documents/Apps/Quibit_RAG/ET_v001/mcp-server-asana/node_modules/.pnpm/jest-cli@29.7.0_@types+node@20.19.1/node_modules/jest-cli/node_modules:/Users/adamhayden/Documents/Apps/Quibit_RAG/ET_v001/mcp-server-asana/node_modules/.pnpm/jest-cli@29.7.0_@types+node@20.19.1/node_modules:/Users/adamhayden/Documents/Apps/Quibit_RAG/ET_v001/mcp-server-asana/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/adamhayden/Documents/Apps/Quibit_RAG/ET_v001/mcp-server-asana/node_modules/.pnpm/jest-cli@29.7.0_@types+node@20.19.1/node_modules/jest-cli/bin/node_modules:/Users/adamhayden/Documents/Apps/Quibit_RAG/ET_v001/mcp-server-asana/node_modules/.pnpm/jest-cli@29.7.0_@types+node@20.19.1/node_modules/jest-cli/node_modules:/Users/adamhayden/Documents/Apps/Quibit_RAG/ET_v001/mcp-server-asana/node_modules/.pnpm/jest-cli@29.7.0_@types+node@20.19.1/node_modules:/Users/adamhayden/Documents/Apps/Quibit_RAG/ET_v001/mcp-server-asana/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jest-cli/bin/jest.js" "$@"
else
  exec node  "$basedir/../jest-cli/bin/jest.js" "$@"
fi
