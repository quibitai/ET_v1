#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/adamhayden/Documents/Apps/Quibit_RAG/ET_v001/mcp-server-asana/node_modules/.pnpm/@babel+cli@7.27.2_@babel+core@7.27.4/node_modules/@babel/cli/bin/node_modules:/Users/adamhayden/Documents/Apps/Quibit_RAG/ET_v001/mcp-server-asana/node_modules/.pnpm/@babel+cli@7.27.2_@babel+core@7.27.4/node_modules/@babel/cli/node_modules:/Users/adamhayden/Documents/Apps/Quibit_RAG/ET_v001/mcp-server-asana/node_modules/.pnpm/@babel+cli@7.27.2_@babel+core@7.27.4/node_modules/@babel/node_modules:/Users/adamhayden/Documents/Apps/Quibit_RAG/ET_v001/mcp-server-asana/node_modules/.pnpm/@babel+cli@7.27.2_@babel+core@7.27.4/node_modules:/Users/adamhayden/Documents/Apps/Quibit_RAG/ET_v001/mcp-server-asana/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/adamhayden/Documents/Apps/Quibit_RAG/ET_v001/mcp-server-asana/node_modules/.pnpm/@babel+cli@7.27.2_@babel+core@7.27.4/node_modules/@babel/cli/bin/node_modules:/Users/adamhayden/Documents/Apps/Quibit_RAG/ET_v001/mcp-server-asana/node_modules/.pnpm/@babel+cli@7.27.2_@babel+core@7.27.4/node_modules/@babel/cli/node_modules:/Users/adamhayden/Documents/Apps/Quibit_RAG/ET_v001/mcp-server-asana/node_modules/.pnpm/@babel+cli@7.27.2_@babel+core@7.27.4/node_modules/@babel/node_modules:/Users/adamhayden/Documents/Apps/Quibit_RAG/ET_v001/mcp-server-asana/node_modules/.pnpm/@babel+cli@7.27.2_@babel+core@7.27.4/node_modules:/Users/adamhayden/Documents/Apps/Quibit_RAG/ET_v001/mcp-server-asana/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@babel+cli@7.27.2_@babel+core@7.27.4/node_modules/@babel/cli/bin/babel.js" "$@"
else
  exec node  "$basedir/../../../../../@babel+cli@7.27.2_@babel+core@7.27.4/node_modules/@babel/cli/bin/babel.js" "$@"
fi
